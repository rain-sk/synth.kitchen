{
   "compilerOptions": {
      "target": "es2022",
      "lib": [
         "es2022"
      ],
      "outDir": "./build/",
      "composite": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "moduleResolution": "node"
   }
}